   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Rated electrical omega",
   "optional" : true
  }, {
   "name" : "ratedTorque",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Rated electrical torque",
   "optional" : true
  }, {
