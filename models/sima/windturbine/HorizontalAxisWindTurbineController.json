   "default" : "DEFAULT"
  }, {
   "name" : "gainItems",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "./GainItem",
   "dimensions" : "*",
   "contained" : true,
   "optional" : true
  }, {
   "name" : "external",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "boolean",
   "description" : "Use external controller",
   "optional" : true
  }, {
   "name" : "controllerFile",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "Path to controller file",
   "optional" : true
  }, {
   "name" : "className",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "Class name of controller",
   "optional" : true
  }, {
   "name" : "configuration",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "Configuration filename",
   "optional" : true
  }, {
   "name" : "libraryPaths",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "sima:sima/LibraryPaths",
   "contained" : true,
   "optional" : false
  }, {
