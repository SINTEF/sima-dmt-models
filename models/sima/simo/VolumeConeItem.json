{
 "name" : "VolumeConeItem",
 "type" : "core:Blueprint",
 "extends" : ["./VolumeMassPortion"],
 "attributes" : [{
   "name" : "volume",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "enumType" : "./Volume",
   "description" : "Add or subtract volume",
   "optional" : true,
   "default" : "ADD"
  }, {
   "name" : "centerPoint",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "sima:sima/Point3",
   "contained" : true,
   "optional" : false,
   "description" : "Location of center of bottom plane in local coordinates"
  }, {
   "name" : "length",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Length of cone",
   "optional" : true
  }, {
   "name" : "diameterBottomPlane",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Diameter of plane 1 (bottom plane)",
   "optional" : true
  }, {
   "name" : "diameterTopPlane",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Diameter of plane 2",
   "optional" : true
  }, {
   "name" : "axis",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "enumType" : "./Axis",
   "description" : "Cone axis direction:\r\nX: parallell to Tank x-axis\r\nY: parallell to Tank y-axis\r\nZ: parallell to Tank z-axis",
   "optional" : true,
   "default" : "Z"
  }]
}
