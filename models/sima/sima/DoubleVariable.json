{
 "name" : "DoubleVariable",
 "type" : "core:Blueprint",
 "extends" : ["./Variable"],
 "attributes" : [{
   "name" : "name",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "optional" : true
  }, {
   "name" : "value",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "The current value for the variable",
   "optional" : true
  }, {
   "name" : "unit",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "unit of variable",
   "optional" : true,
   "default" : "-"
  }]
}
