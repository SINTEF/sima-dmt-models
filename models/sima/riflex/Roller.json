{
 "name" : "Roller",
 "type" : "core:Blueprint",
 "extends" : ["sima:sima/NamedObject"],
 "attributes" : [{
   "name" : "name",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "optional" : true
  }, {
   "name" : "direction",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Direction of roller axis (Clockwise around the local X-axis of the actual surface plane).",
   "optional" : true,
   "default" : 0.0
  }, {
   "name" : "y",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Y-coordinate of roller origin",
   "optional" : true,
   "default" : 0.0
  }, {
   "name" : "z",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Z-coordinate of roller origin",
   "optional" : true,
   "default" : 0.0
  }, {
   "name" : "length",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Length of roller (0 means infinite length)",
   "optional" : true,
   "default" : 0.0
  }, {
   "name" : "constantStiffness",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "boolean",
   "description" : "Constant spring compression stiffness.",
   "optional" : true
  }, {
   "name" : "damping",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Dash pot damping coefficient",
   "optional" : true,
   "default" : 0.0
  }, {
   "name" : "stiffness",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Spring compression stiffness",
   "optional" : true
  }, {
   "name" : "radius",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "number",
   "description" : "Radius of roller",
   "optional" : true
  }, {
   "name" : "stiffnessItems",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "./SpringStiffnessItem",
   "dimensions" : "*",
   "contained" : true,
   "optional" : true
  }]
}
