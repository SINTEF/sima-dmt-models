{
 "name" : "ExternalCommand",
 "type" : "core:Blueprint",
 "extends" : ["./Command"],
 "attributes" : [{
   "name" : "name",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "optional" : true
  }, {
   "name" : "parameters",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "sima:sima/Property",
   "dimensions" : "*",
   "contained" : true,
   "optional" : true,
   "description" : "Additional parameters"
  }, {
   "name" : "file",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "file path to command file (*.txt or *.json)",
   "optional" : false
  }, {
   "name" : "ignore",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "boolean",
   "description" : "Continute command despite any intermediate failure",
   "optional" : true
  }]
}
