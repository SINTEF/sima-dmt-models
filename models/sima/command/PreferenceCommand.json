{
 "name" : "PreferenceCommand",
 "type" : "core:Blueprint",
 "extends" : ["./Command"],
 "attributes" : [{
   "name" : "name",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "optional" : true
  }, {
   "name" : "parameters",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "sima:sima/Property",
   "dimensions" : "*",
   "contained" : true,
   "optional" : true,
   "description" : "Additional parameters"
  }, {
   "name" : "file",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "string",
   "description" : "Path to file",
   "optional" : false
  }, {
   "name" : "export",
   "type" : "core:BlueprintAttribute",
   "attributeType" : "boolean",
   "description" : "If checked, this will export the preferences instead of importing",
   "optional" : true
  }]
}
